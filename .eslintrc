{
    "env": {
        "es6": true,
        "node": true
    },
    "extends": [
        "eslint:recommended",
        "prettier"
    ],
    "parserOptions": {
        "ecmaVersion": 2017
    },
    "rules": {
        "no-console": "off",
        "prettier/prettier": [
            "error",
            {
                "singleQuote": true
            }
        ]
    },
    "plugins": [
        "prettier"
    ]
}